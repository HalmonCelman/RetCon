# some simple program to test if LLL is working on RetCon
mov r0, @0
mov r1, @0
mov r2, @4
mov r3, @4

:infloop
    out @0 2    # clr screen
    
    # manage
    in r4 1

    ands r5 r4 @1   # TL pressed

    # draw char A
    outzc @0 5
    outzc @11 5
    outzc @65 5
    outzc @0 1    # refresh

frjmp :infloop 0

